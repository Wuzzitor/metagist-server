{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Package</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Identifier</th>
                <td>{{ entity.identifier }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Versions</th>
                <td>{{ entity.versions }}</td>
            </tr>
            <tr>
                <th>Timeupdated</th>
                <td>{{ entity.timeUpdated|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Overallrating</th>
                <td>{{ entity.overallRating }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_packages') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_packages_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
