{% extends 'MetagistServerBundle::layout.html.twig' %}
{% set pagetitle = 'Package' %}

{% block content -%}
<div class="row padded default-gradient">
    
<h1>Package</h1>

<table class="table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Identifier</th>
            <td>{{ entity.identifier }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ entity.description }}</td>
        </tr>
        <tr>
            <th>Type</th>
            <td>{{ entity.type }}</td>
        </tr>
        <tr>
            <th>Versions</th>
            <td>{{ entity.versions | join(',')}}</td>
        </tr>
        <tr>
            <th>Timeupdated</th>
            <td>{{ entity.timeUpdated|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Overallrating</th>
            <td>{{ entity.overallRating }}</td>
        </tr>
    </tbody>
</table>

<a class="btn btn-default" href="{{ path('admin_packages') }}">
    Back to the list
</a>
<a class="btn btn-default" href="{{ path('admin_packages_edit', { 'id': entity.id }) }}">
    Edit
</a>
{{ form(delete_form) }}
</div>
{% endblock %}
