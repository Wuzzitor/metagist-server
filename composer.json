{
   "name": "symfony\/framework-standard-edition",
   "license": "MIT",
   "type": "project",
   "version": "0.0.0",
   "description": "The \"Symfony Standard Edition\" distribution",
   "autoload": {
      "psr-0": {
         "_empty_": "src\/",
         "Metagist\\Api": "vendor\/wuzzitor\/metagist-api\/src"
      }
   },
   "require": {
      "php": ">=5.3.3",
      "symfony\/symfony": "2.3.*",
      "doctrine\/orm": ">=2.2.3,<2.4-dev",
      "doctrine\/doctrine-bundle": "1.2.*",
      "twig\/extensions": "1.0.*",
      "symfony\/assetic-bundle": "2.3.*",
      "symfony\/swiftmailer-bundle": "2.3.*",
      "symfony\/monolog-bundle": "2.3.*",
      "sensio\/distribution-bundle": "2.3.*",
      "sensio\/framework-extra-bundle": "2.3.*",
      "sensio\/generator-bundle": "2.3.*",
      "incenteev\/composer-parameter-handler": "~2.0",
      "hwi\/oauth-bundle": "0.3.*@dev",
      "twitter\/bootstrap": "dev-3.0.0-wip",
      "knplabs\/packagist-api": "0.1.*@dev",
      "pagerfanta\/pagerfanta": "v1.0.0",
      "bonndan\/ReleaseManager": "0.2.1",
      "wuzzitor\/metagist-api": "0.0.5"
   },
   "require-dev": {
      "fzaninotto\/faker": "v1.2.0"
   },
   "scripts": {
      "post-install-cmd": [
         "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
         "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
         "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
         "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
         "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
      ],
      "post-update-cmd": [
         "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
         "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
         "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
         "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
         "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
      ]
   },
   "config": {
      "bin-dir": "bin"
   },
   "minimum-stability": "beta",
   "extra": {
      "symfony-app-dir": "app",
      "symfony-web-dir": "web",
      "incenteev-parameters": {
         "file": "app\/config\/parameters.yml"
      },
      "branch-alias": {
         "dev-master": "2.3-dev"
      },
      "rmt": {
         "vcs": "git",
         "prerequisites": ["working-copy-check", "display-last-changes"],
         "preReleaseActions": [
              {
                  "name": "version-stamp",
                  "const": "METAGIST_VERSION",
                  "file": "src/Metagist/ServerBundle/version.php"
              },
              "composer-update",
              {
                "name": "changelog-update",
                "format": "semantic",
                "dump-commits": true
              },
              "vcs-commit"
         ],
         "versionPersister": "changelog",
         "postReleaseActions": [
            
         ]
      }
   },
   "repositories": [
      {
         "type": "vcs",
         "url": "https:\/\/github.com\/Wuzzitor\/metagist-api.git"
      },
      {
         "type": "package",
         "package": {
            "name": "bonndan\/ReleaseManager",
            "version": "0.2.1",
            "source": {
               "url": "http:\/\/github.com\/bonndan\/ReleaseManager.git",
               "type": "git",
               "reference": "0.2.1"
            }
         }
      }
   ]
}